<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="3"
    android:orientation="horizontal"
	android:background="#436EEE"
	>
    <Button 
            android:id="@+id/objinsert_text"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/button_style"
            android:button="@null"
	        android:layout_gravity="center"
	        android:text="@string/object_text"
	        android:textSize="40sp"
	        android:textColor="#000000"
            />
        
    <Button 
            android:id="@+id/objinsert_time"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_clock"
            android:button="@null"
	        android:layout_gravity="center"
            />
        
    <Button 
            android:id="@+id/objinsert_counter"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_cnt"
            android:button="@null"
	        android:layout_gravity="center"
            />
        
    <Button 
            android:id="@+id/objinsert_barcode"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_barcode"
            android:button="@null"
	        android:layout_gravity="center"
            />
        
    <Button 
            android:id="@+id/objinsert_julian"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_julian"
            android:button="@null"
	        android:layout_gravity="center"
            />
    <Button
            android:id="@+id/objinsert_graphic"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_image"
            android:button="@null"
	        android:layout_gravity="center"
            />
        
    <Button
            android:id="@+id/objinsert_line"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_line"
            android:button="@null"
	        android:layout_gravity="center"
            />
    <Button
            android:id="@+id/objinsert_rect"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_rect"
            android:button="@null"
	        android:layout_gravity="center"
            />
    <Button
            android:id="@+id/objinsert_ellipse"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/style_ellipse"
            android:button="@null"
	        android:layout_gravity="center"
            />

</GridLayout>
